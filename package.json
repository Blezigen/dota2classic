{
  "name": "electron-webpack-quick-start",
  "version": "0.0.2",
  "license": "MIT",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "CSC_LINK=didc.cer CSC_IDENTITY_AUTO_DISCOVERY=true yarn compile && electron-builder",
    "dist:dir": "CSC_LINK=didc.cer CSC_IDENTITY_AUTO_DISCOVERY=true yarn dist --dir -c.compression=store -c.mac.identity=null"
  },
  "dependencies": {
    "bignumber.js": "^9.0.1",
    "classnames": "^2.2.6",
    "dataurl": "^0.1.0",
    "greenworks": "git+https://github.com/greenheartgames/greenworks.git",
    "mobx": "^5.11.0",
    "mobx-react": "^6.2.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "socket.io-client": "^2.3.1",
    "source-map-support": "^0.5.16",
    "styled-components": "^5.2.0",
    "update-electron-app": "^2.0.1"
  },
  "build": {
    "appId": "ru.dota2classic.app",
    "productName": "dota2classic",
    "win": {
      "icon": "./static/logo.png",
      "target": [
        "nsis"
      ]
    },
    "mac": {
      "icon": "./static/logo.png"
    },
    "extraFiles": [
      "steam_appid.txt"
    ]
  },
  "devDependencies": {
    "@babel/preset-react": "^7.12.1",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/socket.io-client": "^1.4.34",
    "@types/styled-components": "^5.1.4",
    "electron": "4.2.12",
    "electron-builder": "^22.4.1",
    "electron-rebuild": "^2.2.0",
    "electron-webpack": "^2.8.2",
    "electron-webpack-ts": "^4.0.1",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3",
    "webpack": "~4.42.1"
  },
  "electronWebpack": {
    "whiteListedModules": [
      "react",
      "react-dom",
      "mobx-react",
      "styled-components"
    ],
    "renderer": {
      "template": "src/index.html"
    }
  }
}
